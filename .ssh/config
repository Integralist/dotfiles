Host ssh dev.buzzfeed.io
  ForwardAgent yes
  User root
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null

Host gitlab.com
  User git
  HostName gitlab.com
  IdentityFile ~/.ssh/gitlab_rsa

Host mng02.buzzfeed.com
  Hostname mng02.buzzfeed.com
  port 43821
  User mark.mcdonnell
  IdentityFile ~/.ssh/bf_rsa

Host newsiege01
  User mark.mcdonnell
  IdentityFile ~/.ssh/bf_rsa
  ForwardX11 yes
  ServerAliveInterval 290
  ForwardAgent yes
  hostname ec2-54-161-254-149.compute-1.amazonaws.com
  ProxyCommand ssh mng02.buzzfeed.com nc %h %p

